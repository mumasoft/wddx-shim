{
  "name": "mumasoft/wddx-shim",
  "description": "WDDX shim for PHP >= 7.4",
  "license": "MIT",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Johan Mulder",
      "email": "johan@mumasoft.nl"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.4|8.*",
    "ext-dom": "*",
    "ext-xml": "*"
  },
  "require-dev": {
